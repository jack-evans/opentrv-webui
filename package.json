{
  "name": "my-app",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "autoprefixer": "7.1.2",
    "babel-core": "6.25.0",
    "babel-eslint": "7.2.3",
    "babel-jest": "^21.2.0",
    "babel-loader": "^7.1.2",
    "babel-preset-react-app": "^3.0.2",
    "babel-runtime": "6.26.0",
    "bluebird": "^3.5.1",
    "body-parser": "^1.18.2",
    "bunyan": "^1.8.12",
    "carbon-addons-bluemix": "^0.2.0",
    "carbon-components": "^8.1.13",
    "carbon-components-react": "^5.0.11",
    "carbon-icons": "^6.1.0",
    "case-sensitive-paths-webpack-plugin": "2.1.1",
    "chalk": "1.1.3",
    "cloudant": "^1.8.0",
    "css-loader": "0.28.4",
    "dotenv": "4.0.0",
    "eslint": "4.4.1",
    "eslint-config-react-app": "^2.0.0",
    "eslint-loader": "1.9.0",
    "eslint-plugin-flowtype": "2.35.0",
    "eslint-plugin-import": "2.7.0",
    "eslint-plugin-jsx-a11y": "5.1.1",
    "eslint-plugin-react": "7.1.0",
    "express": "^4.16.2",
    "extract-text-webpack-plugin": "3.0.0",
    "file-loader": "0.11.2",
    "fs-extra": "3.0.1",
    "html-webpack-plugin": "2.29.0",
    "isomorphic-fetch": "^2.2.1",
    "object-assign": "4.1.1",
    "postcss-flexbugs-fixes": "3.2.0",
    "postcss-loader": "2.0.6",
    "promise": "8.0.1",
    "react": "^16.2.0",
    "react-dev-utils": "^4.0.0",
    "react-dom": "^16.2.0",
    "react-router-dom": "^4.2.2",
    "request": "^2.83.0",
    "request-promise": "^4.2.2",
    "style-loader": "0.18.2",
    "sw-precache-webpack-plugin": "0.11.4",
    "url-loader": "0.5.9",
    "uuid": "^3.1.0",
    "webpack": "3.5.1",
    "webpack-dev-server": "2.7.1",
    "webpack-manifest-plugin": "1.2.1",
    "whatwg-fetch": "2.0.3"
  },
  "scripts": {
    "lint": "standard --verbose | snazzy",
    "prestart": "yarn lint",
    "prebuild": "yarn lint",
    "pretest": "yarn lint",
    "server": "node back-end/server.js",
    "start": "concurrently \"node scripts/start.js\" \"yarn server\"",
    "build": "node scripts/build.js",
    "test": "node scripts/test.js --env=jsdom --coverage",
    "test-no-coverage": "node scripts/test.js --env=jsdom",
    "test-travis": "yarn lint && node scripts/test.js --env=jsdom --coverage --updateSnapshot",
    "bundle-analysis": "webpack --config './config/webpack.config.prod.js' --json > stat.json"
  },
  "proxy": "http://localhost:3001",
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}",
      "back-end/lib/**/*.js"
    ],
    "coveragePathIgnorePatterns": [
      "/node_modules/",
      "/src/registerServiceWorker.js"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 80,
        "functions": 80,
        "lines": 80,
        "statements": 80
      }
    },
    "setupFiles": [
      "<rootDir>/config/polyfills.js"
    ],
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.js?(x)",
      "<rootDir>/src/**/?(*.)(spec|test).js?(x)",
      "<rootDir>/back-end/test/**/*-test.js"
    ],
    "testEnvironment": "node",
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.(js|jsx)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx)$"
    ],
    "moduleNameMapper": {
      "^react-native$": "react-native-web"
    },
    "moduleFileExtensions": [
      "web.js",
      "js",
      "json",
      "web.jsx",
      "jsx",
      "node"
    ]
  },
  "babel": {
    "presets": [
      "react-app"
    ]
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "devDependencies": {
    "concurrently": "^3.5.0",
    "enzyme": "^3.2.0",
    "enzyme-adapter-react-16": "^1.1.0",
    "events": "^1.1.1",
    "jest": "22.0.4",
    "jest-tobetype": "^1.1.0",
    "nock": "^9.1.6",
    "node-mocks-http": "^1.6.6",
    "node-sass": "^4.5.3",
    "react-test-renderer": "^16.2.0",
    "sass-loader": "^6.0.6",
    "snazzy": "^7.0.0",
    "standard": "^11.0.0",
    "svgxuse": "^1.2.4"
  },
  "standard": {
    "env": [
      "jest",
      "node",
      "react"
    ]
  }
}
